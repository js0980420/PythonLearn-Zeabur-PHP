{
    "name": "pythonlearn/collaboration-platform",
    "description": "Python 教學多人協作平台",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "backend/classes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "*": true
        },
        "platform-check": false,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
